<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="10.0" xmlns="http://javafx.com/javafx/23" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ps.managmenrt.airport.DeleteStaffPage_controller">
    <padding>
        <Insets top="20.0" right="20.0" bottom="20.0" left="20.0"/>
    </padding>
    <Label text="Delete Staff Member" style="-fx-font-size: 24px; -fx-text-fill: #333333;" />
    <TextField fx:id="usernameField" promptText="Enter Username to Delete" />
    <Button text="Delete" onAction="#onDeleteButtonClick" />
    <Button text="Back" onAction="#goBack" />
</VBox>

