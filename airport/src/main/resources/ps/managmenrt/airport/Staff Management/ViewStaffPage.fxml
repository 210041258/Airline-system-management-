<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="BOTTOM_CENTER" prefHeight="427.0" prefWidth="871.0" spacing="20.0" xmlns="http://javafx.com/javafx/23" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ps.managmenrt.airport.ViewStaffPage_controller">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <TableView fx:id="staffTable">
        <columns>
            <TableColumn text="Email" fx:id="emailColumn" />
            <TableColumn text="Username" fx:id="usernameColumn" />
            <TableColumn text="Salary" fx:id="salaryColumn" />
            <TableColumn text="Job Position" fx:id="jobPositionColumn" />
        </columns>
    </TableView>
    <Button text="Add Staff" onAction="#onAddStaffClick" />
    <Button text="Update Staff" onAction="#onUpdateStaffClick" />
    <Button text="Delete Staff" onAction="#onDeleteStaffClick" />
    <Button text="Back" onAction="#goBack" />

</VBox>
