<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<?import javafx.scene.control.TextField?>
<VBox alignment="BOTTOM_CENTER" prefHeight="427.0" prefWidth="871.0" spacing="20.0" xmlns="http://javafx.com/javafx/23" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ps.managmenrt.airport.add_balance_controller">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>

    <Label text="Update Balance" />
    <TextField fx:id="usernameField" promptText="Username" />
    <TextField fx:id="balanceField" promptText="New Balance" />
    <Button text="Update Balance" onAction="#onUpdateBalanceClick" />
    <Button text="Back" onAction="#onBackClick" />
</VBox>
