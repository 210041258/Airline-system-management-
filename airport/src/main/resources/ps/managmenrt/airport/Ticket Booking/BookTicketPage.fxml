<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="TOP_CENTER" prefHeight="400.0" prefWidth="600.0" spacing="20.0" xmlns="http://javafx.com/javafx/23" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ps.managmenrt.airport.BookTicketPage_controller">
    <padding>
        <Insets top="20" right="20" bottom="20" left="20" />
    </padding>

    <ComboBox fx:id="flightComboBox" promptText="Select Flight" />
    <Button fx:id="buyTicketButton" text="Buy Ticket" onAction="#onBuyTicketClick" />
    <Button text="Back" onAction="#goBack" />
</VBox>
