<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="TOP_CENTER" prefHeight="400.0" prefWidth="600.0" spacing="20.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ps.managmenrt.airport.BookTicketPage_controller">
    <padding>
        <Insets bottom="20" left="20" right="20" top="20" />
    </padding>

    <Label style="-fx-font-size: 20; -fx-font-weight: bold;" text="Ticket Information" />

    <ComboBox fx:id="flightComboBox" promptText="Select Ticket" />

    <Button fx:id="buyTicketButton" onAction="#onBuyTicketClick" text="Buy Ticket" />
    <Button onAction="#goBack" text="Back" />
   <Label fx:id="staus" style="-fx-font-size: 20; -fx-font-weight: bold;" text=".." />
</VBox>
